{
    "[lua]": {
        "editor.defaultFormatter": "sumneko.lua"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "editor.tabSize": 4,
    "workbench.colorCustomizations": {
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#635485",
        "statusBar.background": "#4c4066",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#635485",
        "statusBarItem.remoteBackground": "#4c4066",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "tab.activeBorder": "#635485",
        "titleBar.activeBackground": "#4c4066",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#4c406699",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#4c4066",
    "wowAPI.luals.defineKnownGlobals": false,
    "Lua.completion.autoRequire": false,
    "explorer.sortOrder": "default",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": true,
    "explorer.fileNesting.patterns": {
        "*.lua": "${capture}.lua, ${capture}.xml",
        "*.toc": ".pkgmeta, .luarc.json, README.md, .gitignore, annotations.lua"
    },
    "Lua.runtime.version": "Lua 5.1",
    "Lua.runtime.builtin": {
        "basic": "disable",
        "debug": "disable",
        "io": "disable",
        "math": "disable",
        "os": "disable",
        "package": "disable",
        "string": "disable",
        "table": "disable",
        "utf8": "disable"
    },
    "Lua.workspace.ignoreDir": [
        ".github",
        ".vscode",
        "libs"
    ],
    "Lua.diagnostics.disable": [
        "redefined-local",
        "assign-type-mismatch",
        "undefined-field",
    ],
    "Lua.diagnostics.neededFileStatus": {
        "global-element": "Any",
        "missing-global-doc": "Any",
        "codestyle-check": "Any",
        "incomplete-signature-doc": "Any",
        "no-unknown": "Any"
    },
    "Lua.diagnostics.groupSeverity": {
        "global": "Error"
    },
    "Lua.diagnostics.globals": [
        "TomTom",
        "ScrollFrame_OnLoad",
        "StaticPopupDialogs"
    ],
    "Lua.workspace.library": [
        "libs\\NercUtils",
        "~/.vscode/extensions/ketho.wow-api-0.20.8/Annotations/Core",
        "~/.vscode/extensions/ketho.wow-api-0.20.8/Annotations/FrameXML"
    ],
    "dryerLint.ruleSets": [
        {
            "name": "WoW Addon Code Styling",
            "language": "lua",
            "glob": "{*.lua,**/!(libs)/**/*.lua}",
            "rules": [
                {
                    // "name" is a string used to identify the rule
                    "name": "NoPrint",
                    "pattern": "\\bprint\\s*\\(",
                    "message": "Don't use print in production code.",
                    "fix": "",
                    "severity": "Error",
                },
            ]
        },
    ],
    "editor.formatOnSave": true,
    "editor.indentSize": "tabSize",
    "editor.tabCompletion": "onlySnippets",
    "Lua.diagnostics.groupFileStatus": {
        "ambiguity": "Any",
        "await": "Any",
        "duplicate": "Any",
        "global": "Any",
        "luadoc": "Any",
        "redefined": "Any",
        "strict": "Any",
        "strong": "Any",
        "type-check": "Any",
        "unbalanced": "Any",
        "unused": "Any"
    },
    "Lua.diagnostics.libraryFiles": "Disable",
    "Lua.format.defaultConfig": {
        "indent_size": "4",
        "indent_style": "space"
    },
    "Lua.hint.setType": false,
    "material-icon-theme.folders.associations": {
        "panel": "Template",
        "pins": "Taskfile",
        "sets": "Queue",
        "tracker": "Ui"
    },
    "Lua.type.weakUnionCheck": true,
}
